<% case search_result[:record_type] %>
<% when  /heading|accepted|excluded|synonym|misapplied|in-batch-note|in-batch-compiler-note|distribution|concept-note/ %>
  <%= render partial: "application/search_results/print/records/loader/record_types/#{search_result[:record_type].gsub(/-/,'_')}", locals: {search_result: search_result} %>
<% else %>
   <%= render partial: 'application/search_results/print/records/loader/record_types/other', locals: {search_result: search_result} %>
<% end %>


